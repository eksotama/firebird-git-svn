# Are we going to compile the SINIX-2000 version?
ifneq ($(APC),)
APC_FLAGS=-I$(APC)/Headers
endif
COMMON_FLAGS=-DSINIXZ -DFB_SEND_FLAGS=SO_NOSIGPIPE $(APC_FLAGS)

PROD_WARNINGS=-w
DEV_WARNINGS=-Wall -W -Wno-unused -Wno-parentheses -Wno-switch -Wno-unknown-pragmas
#DEV_WARNINGS=-Wall -W -Wno-parentheses -Wno-switch

PROD_FLAGS=$(PROD_WARNINGS) -DNDEBUG $(COMMON_FLAGS) -pipe -fPIC
DEV_FLAGS=$(DEV_WARNINGS) -ggdb $(COMMON_FLAGS) -pipe -fPIC

OS_ServerFiles=inet_server.cpp

EMBED_UTIL_TARGETS=gstat gds_drop gds_relay gsec nbackup fb_lock_print
CLIENT_UTIL_TARGETS=gds_drop gds_relay gstat gsec fbguard fbmgr_bin nbackup fb_lock_print

Physical_IO_Module=os/posix/unix.cpp

