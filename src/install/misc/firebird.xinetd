# default: on
# description: FirebirdSQL server
service gds_db
{
	flags			= REUSE
	socket_type		= stream
	wait			= no
#	user			= @FBRunUser@
	user			= root
	log_on_success		+= USERID
	log_on_failure 		+= USERID
	server			= /usr/local/firebird/bin/fb_inet_server
#	server			= ${exec_prefix}/sbin/gds_inet_server
#	server			= @exec_bindir@/gds_inet_server
    disable         = no
}
